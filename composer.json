{
    "name": "xiaomlove/grid-exporter",
    "description": "export grid data for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "export"],
    "homepage": "https://github.com/xiaomlove/grid-exporter",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaomlove",
            "email": "353856593@qq.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "encore/laravel-admin": "dev-customize-2",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Chenyulingxi\\LaravelAdmin\\GridExporter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chenyulingxi\\LaravelAdmin\\GridExporter\\GridExporterServiceProvider"
            ]

        }
    },
    "repositories": {
        "laravel-admin":  {
            "type": "git",
            "url": "https://github.com/rechik/laravel-admin.git"
        }
    }
}
